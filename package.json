    "antd": "^5.3.2",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
